<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="jp.ac.tokushima_u.is.ll.dao.PacallItemRecordDao">
	<insert id="insert" parameterType="PacallItemRecord">
		insert into pacall_item_record(
			id,
			item_id,
			photo_id,
			know,
			what
		)values(
			#{id},
			#{itemId},
			#{photoId},
			#{know},
			#{what}
		)
	</insert>
</mapper>